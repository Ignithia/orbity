{
  "name": "orbity",
  "version": "1.1.0",
  "description": "Interactive 3D tag cloud library for web apps (TypeScript, ESM/CJS, React/Vue wrappers)",
  "main": "dist/orbity.cjs.js",
  "module": "dist/orbity.esm.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/orbity.esm.js",
      "require": "./dist/orbity.cjs.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist/",
    "index.d.ts",
    "src/",
    "wrappers/"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "lint": "eslint src/ wrappers/ --ext .ts,.tsx,.vue",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ignithia/orbity"
  },
  "keywords": [
    "3d",
    "tagcloud",
    "visualization",
    "typescript",
    "react",
    "vue",
    "canvas",
    "accessibility"
  ],
  "author": "Ignithia <contact@ryanvdv-portfolio.be>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.29.0",
    "react": "^19.1.0",
    "rollup": "^4.44.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1",
    "vue": "^3.5.17"
  },
  "peerDependencies": {
    "react": ">=17",
    "vue": ">=3"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "react-dom": "^19.1.0",
    "tslib": "^2.8.1"
  }
}
